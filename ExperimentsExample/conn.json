    },
    {
      "name": "publicKey",
      "value": "replace-me"
    },
    {
      "name": "secretKey",
      "value": "#SECURE.awssecretadmin"
    },
    {
      "name": "uri",
      "value": "s3a://cortex-fabric-examples/german_credit_eval.csv"
    }
  ]
}