{
  "name": "@c12e/jupyterlab-cortex",
  "version": "0.5.6",
  "description": "A JupyterLab extension by CognitiveScale",
  "repository": {
    "type": "git",
    "url": "https://github.com/CognitiveScale/jupyterlab-cortex.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cognitivescale",
    "cortex",
    "studio",
    "ai",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/CognitiveScale/jupyterlab-cortex",
  "license": "Apache-2.0",
  "author": "CognitiveScale",
  "files": [
    "lib/**/*.{css,d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "assets/**/*.{eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc && node-sass --quiet --recursive ./src --output ./lib",
    "clean": "rimraf lib && rimraf node_modules",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "test": "./node_modules/.bin/jest --passWithNoTests",
    "version": "./scripts/generate-python-version.sh && git add jupyterlab_cortex/_version.py",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "0.19.1",
    "@jupyterlab/apputils": "0.19.1",
    "@jupyterlab/coreutils": "2.2.1",
    "@jupyterlab/mainmenu": "0.8.1",
    "@jupyterlab/notebook": "0.19.2",
    "@jupyterlab/services": "3.2.1",
    "@phosphor/datagrid": "0.1.6",
    "@phosphor/messaging": "1.2.2",
    "@phosphor/widgets": "1.6.0",
    "ag-grid": "18.1.2",
    "ag-grid-react": "18.1.0",
    "debug": "4.1.1",
    "lodash": "4.17.11",
    "rc-tree": "1.14.3",
    "react": "~16.4.2",
    "typed-rest-client": "1.2.0"
  },
  "devDependencies": {
    "@types/debug": "4.1.2",
    "@types/jest": "24.0.11",
    "@types/lodash": "4.14.123",
    "@types/react": "~16.4.16",
    "@types/react-dom": "~16.0.9",
    "jest": "24.5.0",
    "node-sass": "4.11.0",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.0",
    "tslint": "5.14.0",
    "tslint-react": "3.6.0",
    "typescript": "^3.1.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "jupyterlab": {
    "extension": true
  }
}
